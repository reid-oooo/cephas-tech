{
  "version": 2,
  "buildCommand": "npm run build",
  "functions": {
    "api/index.js": {
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/dist/client/assets/$1"
    },
    {
      "src": "/images/(.*)",
      "dest": "/public/images/$1"
    },
    {
      "src": "/favicon.ico",
      "dest": "/public/images/cephas.png"
    },
    {
      "src": "/favicon.png",
      "dest": "/public/images/cephas.png"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ]
}
